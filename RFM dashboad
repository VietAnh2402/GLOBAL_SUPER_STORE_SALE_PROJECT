   "cell_type": "code",
   "execution_count": 105,
   "id": "7085540a",
   "metadata": {},
   "outputs": [],
   "source": [
    "rfm_table['Rank_r'] = rfm_table['Recency'].rank()\n",
    "rfm_table['Rank_f'] = rfm_table['Frequency'].rank()\n",
    "rfm_table['Rank_m'] = rfm_table['Moneytary'].rank()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
   "id": "55427f9d",
   "metadata": {},
   "outputs": [],
   "source": [
    "total_rows = len(rfm_table)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 110,
   "id": "49b20e04",
   "metadata": {},
   "outputs": [],
   "source": [
    "rfm_table['pcr_r'] = (rfm_table['Rank_r'] - 1) / (total_rows - 1)\n",
    "rfm_table['pcr_f'] = (rfm_table['Rank_f'] - 1) / (total_rows - 1)\n",
    "rfm_table['pcr_m'] = (rfm_table['Rank_m'] - 1) / (total_rows - 1)"
   ]
