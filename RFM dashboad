   "outputs": [],
   "source": [
    "ecommerce_retail['last_purchase']=ecommerce_retail.groupby('CustomerID')['InvoiceDate'].transform('max')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "806773ff",
   "metadata": {},
   "outputs": [],
   "source": [
    "reference_date = pd.to_datetime(\"2011-12-31\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "2174f533",
   "metadata": {},
   "outputs": [],
   "source": [
    "ecommerce_retail['Recency']= (reference_date - ecommerce_retail['last_purchase']).dt.days"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "71018d53",
   "metadata": {},
   "outputs": [],
   "source": [
    "ecommerce_retail['formatted_date'] = ecommerce_retail['InvoiceDate'].dt.strftime('%d%m%Y').astype(str)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "2882122b",
   "metadata": {},
   "outputs": [],
   "source": [
    "ecommerce_retail['Frequency']= ecommerce_retail.groupby('CustomerID')['formatted_date'].transform('nunique')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "id": "9d363da8",
   "metadata": {},
   "outputs": [],
   "source": [
    "ecommerce_retail['charge_amount']= ecommerce_retail['Quantity']*ecommerce_retail['UnitPrice']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "8efd6a18",
   "metadata": {},
   "outputs": [],
   "source": [
    "ecommerce_retail['Moneytary']= ecommerce_retail.groupby('CustomerID')['charge_amount'].transform('sum')"
   ]
  },
