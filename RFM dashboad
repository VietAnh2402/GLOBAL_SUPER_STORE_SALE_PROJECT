   "source": [
    "def categorize_value(value):\n",
    "    # Convert percentages to their corresponding values\n",
    "    range_20 = 0.20\n",
    "    range_40 = 0.40\n",
    "    range_60 = 0.60\n",
    "    range_80 = 0.80\n",
    "\n",
    "    # Compare the value with percentage ranges\n",
    "    if value < range_20:\n",
    "        return '5'\n",
    "    elif range_20 <= value < range_40:\n",
    "        return '4'\n",
    "    elif range_40 <= value < range_60:\n",
    "        return '3'\n",
    "    elif range_60 <= value < range_80:\n",
    "        return '2'\n",
    "    else:\n",
    "        return '1'\n",
    "\n",
    "\n"
   ]
  },
