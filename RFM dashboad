   "cell_type": "code",
   "execution_count": 131,
   "id": "e4437bed",
   "metadata": {},
   "outputs": [],
   "source": [
    "def categorize_convert_value(value):\n",
    "    # Convert percentages to their corresponding values\n",
    "    champions = ['555', '554', '544', '545', '454', '455', '445']\n",
    "    loyal = ['543', '444', '435', '355', '354', '345', '344', '335']\n",
    "    potential_loyalist = ['553', '551', '552', '541', '542', '533', '532', '531', '452', '451', '442', '441', '431', '453','433', '432', '423', '353', '352', '351', '342', '341', '333', '323']\n",
    "    new_customers = ['512', '511', '422', '421', '412', '411', '311']\n",
    "    promising = ['525', '524', '523', '522', '521', '515', '514', '513', '425','424', '413','414','415', '315', '314', '313']\n",
    "    need_attention = ['535', '534', '443', '434', '343', '334', '325', '324']\n",
    "    about_to_sleep = ['331', '321', '312', '221', '213', '231', '241', '251']\n",
    "    at_risk = ['255', '254', '245', '244', '253', '252', '243', '242', '235', '234', '225', '224', '153', '152', '145', '143', '142', '135', '134', '133', '125', '124']\n",
    "    cannot_lose_them = ['155', '154', '144', '214','215','115', '114', '113']\n",
    "    hibernating_customers = ['332', '322', '233', '232', '223', '222', '132', '123', '122', '212', '211']\n",
    "    lost_customers=['111', '112', '121', '131','141','151']\n",
    "    # Compare the value with percentage ranges\n",
    "    if value in champions:\n",
    "        return 'Champions'\n",
    "    elif value in loyal:\n",
    "        return 'Loyal'\n",
    "    elif value in potential_loyalist:\n",
    "        return 'Potential Loyalist'\n",
    "    elif value in new_customers:\n",
    "        return 'New Customers'\n",
    "    elif value in new_customers:\n",
    "        return 'New Customers'\n",
    "    elif value in new_customers:\n",
    "        return 'New Customers'\n",
    "    elif value in promising:\n",
    "        return 'Promising'\n",
    "    elif value in need_attention:\n",
    "        return 'Need Attention'\n",
    "    elif value in about_to_sleep:\n",
    "        return 'About To Sleep'\n",
    "    elif value in at_risk:\n",
    "        return 'At Risk'\n",
    "    elif value in cannot_lose_them:\n",
    "        return 'Cannot Lose Them'\n",
    "    elif value in hibernating_customers:\n",
    "        return 'Hibernating Customers'\n",
    "    else:\n",
    "        return 'Lost Customers'\n"
   ]
  },
  {
